{
  "name": "personal-chrome-extensions",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["tabs", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2,
  "commands": {
    "open_search_box": {
      "description": "Open search form",
      "suggested_key": {
        "default": "Ctrl+Q",
        "windows": "Ctrl+Q",
        "mac": "Command+Q",
        "chromeos": "Ctrl+Q",
        "linux": "Ctrl+Q"
      }
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab-page.html"
  }
}
